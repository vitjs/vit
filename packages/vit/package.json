{
  "name": "vit",
  "version": "0.1.0",
  "description": "vit",
  "keywords": [
    "vit"
  ],
  "author": "theprimone <yuns.xie@qq.com>",
  "homepage": "https://github.com/vitjs/vit#readme",
  "license": "MIT",
  "main": "dist/node/index.js",
  "scripts": {
    "build": "rm -rf ./dist && concurrently \"npm:build-*\" \"node ../../scripts/copy.js\"",
    "build-node": "tsc -p src/node",
    "build-runtime": "tsc -p src/runtime",
    "dev": "rm -rf ./dist && concurrently \"npm:dev-*\" \"node ../../scripts/copy.js\"",
    "dev-node": "tsc -w -p src/node",
    "dev-runtime": "tsc -w -p src/runtime"
  },
  "publishConfig": {
    "registry": "https://registry.npm.taobao.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitjs/vit.git"
  },
  "bugs": {
    "url": "https://github.com/vitjs/vit/issues"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "mustache": "^4.1.0",
    "react-loadable": "^5.5.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "@types/mkdirp": "^1.0.1",
    "@types/mustache": "^4.1.1",
    "@types/react-loadable": "^5.5.5",
    "@types/react-router": "^5.1.13",
    "@types/react-router-dom": "^5.1.7",
    "concurrently": "^6.0.0",
    "vite": "^2.1.3"
  }
}
